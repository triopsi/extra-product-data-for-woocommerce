{
  "name": "triopsi/extra-product-data-for-woocommerce",
  "homepage": "https://triopsi.com",
  "type": "project",
  "version": "2.0.1",
  "autoload": {
    "psr-4": {
      "Triopsi\\Exprdawc\\": "src/classes/"
    },
    "classmap": [
      "src/classes/"
    ]
  },
  "authors": [
    {
      "name": "Daniel Drevermann",
      "email": "daniel@triopsi.com"
    }
  ],
  "scripts": {
    "test": "phpunit --testdox",
    "test-cov": "XDEBUG_MODE=coverage phpunit --coverage-html=reports/coverage --coverage-clover=reports/clover.xml --testdox",
    "test-install": "bash bin/install-wp-tests.sh wordpress_test root 'root' db latest"
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    },
    "platform": {
      "php": "8.0.0"
    }
  },
  "require": {
    "php": ">=8.0"
  },
  "require-dev": {
    "wp-coding-standards/wpcs": "^3.0",
    "squizlabs/php_codesniffer": "^3.13.5",
    "dealerdirect/phpcodesniffer-composer-installer": "1.0.0",
    "yoast/phpunit-polyfills": "^4.0",
    "phpcompatibility/phpcompatibility-wp": "*",
    "wp-phpunit/wp-phpunit": "^6.3",
    "phpunit/php-code-coverage": "^9.2",
    "phpunit/phpunit": "^9.6"
  }
}
