{
  "name": "extra-product-data-for-woocommerce",
  "version": "2.0.1",
  "description": "Its a Wordpress plugin. Adds customizable input fields per product in WooCommerce.",
  "main": "index.js",
  "scripts": {
    "development": "npm run build:css && vite build",
    "watch": "npm run watch:css & vite build --watch",
    "watch-poll": "npm run watch:css -- --poll=1000 & vite build --watch",
    "build": "npm run build:css && vite build",
    "build:css": "sass src/assets/scss:assets/css --style=compressed",
    "watch:css": "sass --watch src/assets/scss:assets/css --style=expanded",
    "production": "npm run build:css && vite build",
    "prod": "npm run production",
    "prepare": "husky",
    "commit": "commit",
    "test": "composer test",
    "plugin-zip": "mkdir -p release && rsync -av --delete --exclude-from=\".distignore\" ./ release/extra-product-data-for-woocommerce/ && cd release && zip -r extra-product-data-for-woocommerce.zip extra-product-data-for-woocommerce && cd ..",
    "wp-env": "wp-env",
    "wp:start": "wp-env start",
    "wp:stop": "wp-env stop",
    "wp:destroy": "wp-env destroy",
    "wp:cli": "wp-env run cli"
  },
  "author": "triopsi",
  "license": "ISC",
  "devDependencies": {
    "@wordpress/env": "^11.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^10.0.0",
    "eslint-config-wordpress": "^2.0.0",
    "sass": "1.82.0",
    "terser": "^5.46.0",
    "vite": "^7.3.1"
  }
}
